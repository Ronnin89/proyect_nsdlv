$('#card_childs').empty()

    $('#card_childs').append(`
<% @childs.each do |child|%>
                <div class="card mb-3" style="max-width: 540px;">
                    <div class="row no-gutters">
                        <div class="col-md-4">
                        <%= image_tag(child.avatar.variant(resize: '130x130').processed, alt: 'imagen con problemas') if child.avatar.attached? %>
                        </div>
                        <div class="col-md-8">
                        <div class="card-body">
                            <h5 class="card-title">Nombre: <%= child.names %> <%= child.last_names %></h5><hr>
                            <p class="card-text">RUT: <%= child.rut %></p><hr>
                            <p class="card-text">Fecha de nacimiento: <%= child.date_of_birth %></p><hr>
                            <%= link_to "<button>Mas informaci√≥n</button>".html_safe, child %>
                        </div>
                        </div>
                    </div>
                </div>   
<% end%>
                `)