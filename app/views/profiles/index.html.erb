<p id="notice"><%= notice %></p>

<h1>Profiles</h1>

<table>
  <thead>
    <tr>
      <th>Rut</th>
      <th>Names</th>
      <th>Last names</th>
      <th>Date of birth</th>
      <th>Nationality</th>
      <th>Sex</th>
      <th>Address</th>
      <th>Phone</th>
      <th>Type profile</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= profile.rut %></td>
        <td><%= profile.names %></td>
        <td><%= profile.last_names %></td>
        <td><%= profile.date_of_birth %></td>
        <td><%= profile.nationality %></td>
        <td><%= profile.sex %></td>
        <td><%= profile.address %></td>
        <td><%= profile.phone %></td>
        <td><%= profile.type_profile %></td>
        <td><%= link_to 'Show', profile %></td>
        <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
        <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile', new_profile_path %>
